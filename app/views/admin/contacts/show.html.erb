<div class="page-header">
  <%= link_to admin_contacts_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_admin_contact_path(@contact), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show contact</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @contact.name %></dd>

  <dt>Email:</dt>
  <dd><%= @contact.email %></dd>

  <dt>Phone:</dt>
  <dd><%= @contact.phone %></dd>

  <dt>Address:</dt>
  <dd><%= @contact.address %></dd>

  <dt>Store:</dt>
  <dd><%= @contact.store_id %></dd>

</dl>
